{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "html parser",
      "cwd": "${workspaceFolder}",
      "program": "test/html/html_parser.js",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "html codify",
      "cwd": "${workspaceFolder}",
      "program": "test/html/codify.js",
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "html retrieve",
      "cwd": "${workspaceFolder}",
      "program": "test/html/retrieve.js",
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "html transfer",
      "cwd": "${workspaceFolder}",
      "program": "test/html/transfers.js",
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "verbose"
      }
    }
  ]
}
